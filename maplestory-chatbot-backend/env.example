# API Keys (최소한 ANTHROPIC_API_KEY는 필수)
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OPENAI_API_KEY=your_openai_api_key_here  # 선택사항 (없으면 로컬 임베딩 사용)
VOYAGE_API_KEY=your_voyage_api_key_here  # Voyage AI 임베딩용 (권장)

# Claude Settings
CLAUDE_MODEL=claude-sonnet-4-20250514
MAX_TOKENS=4096
TEMPERATURE=0.7

# Vector Store
VECTOR_STORE_TYPE=qdrant
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=
COLLECTION_NAME=maplestory_docs

# 임베딩 설정 (Voyage AI 우선 사용)
EMBEDDING_PROVIDER=auto  # auto, voyage, openai, local
EMBEDDING_MODEL=text-embedding-ada-002  # OpenAI 모델
VOYAGE_MODEL=voyage-3.5-lite  # Voyage AI 모델
LOCAL_EMBEDDING_MODEL=intfloat/multilingual-e5-large  # 로컬 모델
CHUNK_SIZE=1000
CHUNK_OVERLAP=200

# Redis
REDIS_URL=redis://localhost:6379

# Security
CORS_ORIGINS=["http://localhost:3000"]
RATE_LIMIT_PER_MINUTE=10

# FastAPI Settings
APP_NAME=메이플스토리 AI 어시스턴트
APP_VERSION=1.0.0
DEBUG_MODE=false 